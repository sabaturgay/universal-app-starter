{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "isolatedModules": true,
    "baseUrl": "./",
    "paths": {
      "@lib/*": [
        "./lib/*"
      ],
      "@lib": [
        "./lib"
      ],
      "@components/*": [
        "./components/*"
      ],
      "@components": [
        "./components"
      ],
      "@utils": [
        "./utils"
      ],
      "@utils/*": [
        "./utils/*"
      ],
      "@hooks": [
        "./hooks"
      ],
      "@hooks/*": [
        "./hooks/*"
      ],
      "@constants": [
        "./constants"
      ],
      "@assets/*": [
        "./assets/*"
      ],
      "@core/*": [
        "./core/*"
      ],
      "@core": [
        "./core"
      ],
      "@cloud/*": [
        "./cloud/*"
      ],
      "@cloud": [
        "./cloud"
      ],
      "@config/*": [
        "./config/*"
      ],
      "@config": [
        "./config"
      ],
      "@store/*": [
        "./store/*"
      ],
      "@store": [
        "./store"
      ],
      "@screens/*": [
        "./screens/*"
      ],
      "@screens": [
        "./screens"
      ],
      "@type": [
        "./type"
      ],
      "@root": [
        "./"
      ],
      "@root/*": [
        "./*"
      ],
      "@providers": [
        "./providers/"
      ],
      "@providers/*": [
        "./providers/*"
      ]
    },
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "jsx": "preserve",
    "incremental": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
  ]
}
